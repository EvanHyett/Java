package zoo_keeper;

public class Mammal {

//	Member Attributes
	public int energyLevel;
	
//	Constructor
	public Mammal(int energyLevel) {
		this.energyLevel = 100;
	}

//	Methods
	public int displayEnergy(int energy) {
		energyLevel = energy;
		System.out.println(energy);
		return energy;
	}
	
}
